{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ],
    "resources": [
      "model/model.onnx",
      "model/metadata.json"
    ],
    "createUpdaterArtifacts": true
  },
  "productName": "Trackimo Desktop",
  "mainBinaryName": "Trackimo Desktop",
  "version": "1.0.3",
  "identifier": "com.trackimo.desktop",
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://release.trackimo.lol/latest.json",
        "https://github.com/martinctl/trackimo-desktop/releases/latest/download/update-manifest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEI2QzM4MEVGODY1MjhDNjcKUldSbmpGS0c3NEREdHY3WmpZN1VwRHZVcWhBVkZRM0VHaU96a1NlRWlTL09nK09pUU8vYWNDTE8K",
      "windows": {
        "installMode": "basicUi"
      }
    }
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Trackimo Desktop",
        "width": 1200,
        "height": 800,
        "minWidth": 1000,
        "minHeight": 600,
        "useHttpsScheme": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}